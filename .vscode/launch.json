{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug server",
      "program": "${workspaceFolder}/nodejs-demo/dist/index.js",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "outFiles": ["${workspaceFolder}/nodejs-demo/dist/**/*.js"],
      "cwd": "${workspaceFolder}/nodejs-demo",
      "skipFiles": ["<node_internals>/**"],
      "env": {
        "APP_VERSION": "v2",
      }
    }
  ]
}
